{
  "search.exclude": {
    "node_modules/": true,
    "logs/pm2logs/*.log": true,
    "logs/*.log": true,
    "*.log": true,
    "src/server/dist/public/": true,
    "coverage/": true,
    "reports/": true,
    ".nyc_output/": true,
    ".idea/": true,
    "*.css": true,
    "*.css.map": true,
    ".DS_Store": true
  },
  "files.exclude": {
    "node_modules/": true,
    "logs/pm2logs/*.log": true,
    "logs/*.log": true,
    "*.log": true,
    "src/server/dist/public/": true,
    "coverage/": true,
    "reports/": true,
    ".nyc_output/": true,
    ".idea/": true,
    "*.css": true,
    "*.css.map": true,
    ".DS_Store": true
  },
  "[javascript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 2
  },
  "[sass]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[scss]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "eslint.enable": true,
  "stylelint.enable": true,
  "css.validate": false,
  "scss.validate": false
}